{
  "name": "UserAnswer",
  "description": "Individual answers from users",
  "properties": {
    "session_id": {
      "type": "string",
      "description": "ID of the tryout session"
    },
    "question_id": {
      "type": "string",
      "description": "ID of the question"
    },
    "user_answer": {
      "type": "string",
      "description": "User's selected answer (A, B, C, D, E, or null)",
      "enum": ["A", "B", "C", "D", "E"]
    },
    "is_correct": {
      "type": "boolean",
      "description": "Whether the answer is correct"
    },
    "time_spent_seconds": {
      "type": "number",
      "description": "Time spent on this question in seconds"
    }
  }
}